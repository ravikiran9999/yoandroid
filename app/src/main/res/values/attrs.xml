<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Dialpad">
        <attr name="dialpad_key_button_touch_tint" format="color" />
    </declare-styleable>
    <declare-styleable name="ResizingText">
        <attr name="resizing_text_min_size" format="dimension" />
    </declare-styleable>
    <declare-styleable name="NiceSpinner">
        <attr name="arrowTint" format="color" />
        <attr name="hideArrow" format="boolean" />
        <attr name="backgroundSelector" format="integer" />
        <attr name="textTint" format="color" />
    </declare-styleable>

    <!--Custom loader indicator-->
    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>

        <declare-styleable name="YoChatMessageBox">
            <attr name="viewPartMain" format="reference"></attr>
            <attr name="viewPartSlave" format="reference"></attr>
        </declare-styleable>

</resources>